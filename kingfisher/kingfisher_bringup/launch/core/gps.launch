<launch>
  <!-- GPS serial comms -->
  <group ns="gps">
    <node pkg="nmea_comms" type="serial_node" name="serial_node" output="screen">
      <param name="port" value="/dev/ublox" />
      <param name="baud" value="115200" />
    </node>

    <!-- interpretation -->
    <node pkg="nmea_navsat_driver" type="nmea_serial_driver" name="nmea_serial_driver" />

    <!-- ENU translation -->
    <!-- <node pkg="enu" type="enu_publisher.py" name="enu_publisher" /> -->
  </group>
</launch>
